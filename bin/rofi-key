#! /bin/sh
# custom bspwm keybindings cheatsheet
notify-send -u normal -i \
"/usr/share/icons/Papirus-Dark/symbolic/devices/input-keyboard-symbolic.svg" \
"BSPWM keybindings:" \
"S + space	rofi\n\
S +  		term\n\
S + f		Files\n\
S + w		Firefox\n\
S + 1-9,0	go to ws\n\
S + Sh + 1-9,0	mv win to ws\n\
S + Sh + ,   mv win\n\
S + Sh + c	close win\n\
S + x		exit\n\
S + Sh + r	reload bspwm\n\
"
